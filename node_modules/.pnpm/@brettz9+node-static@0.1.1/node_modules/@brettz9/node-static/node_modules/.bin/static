#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/Storage/Local Web/excalibur_demo/node_modules/.pnpm/@brettz9+node-static@0.1.1/node_modules/@brettz9/node-static/bin/node_modules:/Volumes/Storage/Local Web/excalibur_demo/node_modules/.pnpm/@brettz9+node-static@0.1.1/node_modules/@brettz9/node-static/node_modules:/Volumes/Storage/Local Web/excalibur_demo/node_modules/.pnpm/@brettz9+node-static@0.1.1/node_modules/@brettz9/node_modules:/Volumes/Storage/Local Web/excalibur_demo/node_modules/.pnpm/@brettz9+node-static@0.1.1/node_modules:/Volumes/Storage/Local Web/excalibur_demo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Volumes/Storage/Local Web/excalibur_demo/node_modules/.pnpm/@brettz9+node-static@0.1.1/node_modules/@brettz9/node-static/bin/node_modules:/Volumes/Storage/Local Web/excalibur_demo/node_modules/.pnpm/@brettz9+node-static@0.1.1/node_modules/@brettz9/node-static/node_modules:/Volumes/Storage/Local Web/excalibur_demo/node_modules/.pnpm/@brettz9+node-static@0.1.1/node_modules/@brettz9/node_modules:/Volumes/Storage/Local Web/excalibur_demo/node_modules/.pnpm/@brettz9+node-static@0.1.1/node_modules:/Volumes/Storage/Local Web/excalibur_demo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/cli.js" "$@"
else
  exec node  "$basedir/../../bin/cli.js" "$@"
fi
