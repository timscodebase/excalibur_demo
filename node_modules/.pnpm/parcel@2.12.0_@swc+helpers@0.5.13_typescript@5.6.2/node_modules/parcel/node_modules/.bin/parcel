#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/Storage/Local Web/excalibur_demo/node_modules/.pnpm/parcel@2.12.0_@swc+helpers@0.5.13_typescript@5.6.2/node_modules/parcel/lib/node_modules:/Volumes/Storage/Local Web/excalibur_demo/node_modules/.pnpm/parcel@2.12.0_@swc+helpers@0.5.13_typescript@5.6.2/node_modules/parcel/node_modules:/Volumes/Storage/Local Web/excalibur_demo/node_modules/.pnpm/parcel@2.12.0_@swc+helpers@0.5.13_typescript@5.6.2/node_modules:/Volumes/Storage/Local Web/excalibur_demo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Volumes/Storage/Local Web/excalibur_demo/node_modules/.pnpm/parcel@2.12.0_@swc+helpers@0.5.13_typescript@5.6.2/node_modules/parcel/lib/node_modules:/Volumes/Storage/Local Web/excalibur_demo/node_modules/.pnpm/parcel@2.12.0_@swc+helpers@0.5.13_typescript@5.6.2/node_modules/parcel/node_modules:/Volumes/Storage/Local Web/excalibur_demo/node_modules/.pnpm/parcel@2.12.0_@swc+helpers@0.5.13_typescript@5.6.2/node_modules:/Volumes/Storage/Local Web/excalibur_demo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../lib/bin.js" "$@"
else
  exec node  "$basedir/../../lib/bin.js" "$@"
fi
