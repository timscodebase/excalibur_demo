#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/Storage/Local Web/excalibur_demo/node_modules/.pnpm/@excaliburjs+testing@0.26.0_typescript@5.6.2/node_modules/@excaliburjs/testing/dist/src/node_modules:/Volumes/Storage/Local Web/excalibur_demo/node_modules/.pnpm/@excaliburjs+testing@0.26.0_typescript@5.6.2/node_modules/@excaliburjs/testing/dist/node_modules:/Volumes/Storage/Local Web/excalibur_demo/node_modules/.pnpm/@excaliburjs+testing@0.26.0_typescript@5.6.2/node_modules/@excaliburjs/testing/node_modules:/Volumes/Storage/Local Web/excalibur_demo/node_modules/.pnpm/@excaliburjs+testing@0.26.0_typescript@5.6.2/node_modules/@excaliburjs/node_modules:/Volumes/Storage/Local Web/excalibur_demo/node_modules/.pnpm/@excaliburjs+testing@0.26.0_typescript@5.6.2/node_modules:/Volumes/Storage/Local Web/excalibur_demo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Volumes/Storage/Local Web/excalibur_demo/node_modules/.pnpm/@excaliburjs+testing@0.26.0_typescript@5.6.2/node_modules/@excaliburjs/testing/dist/src/node_modules:/Volumes/Storage/Local Web/excalibur_demo/node_modules/.pnpm/@excaliburjs+testing@0.26.0_typescript@5.6.2/node_modules/@excaliburjs/testing/dist/node_modules:/Volumes/Storage/Local Web/excalibur_demo/node_modules/.pnpm/@excaliburjs+testing@0.26.0_typescript@5.6.2/node_modules/@excaliburjs/testing/node_modules:/Volumes/Storage/Local Web/excalibur_demo/node_modules/.pnpm/@excaliburjs+testing@0.26.0_typescript@5.6.2/node_modules/@excaliburjs/node_modules:/Volumes/Storage/Local Web/excalibur_demo/node_modules/.pnpm/@excaliburjs+testing@0.26.0_typescript@5.6.2/node_modules:/Volumes/Storage/Local Web/excalibur_demo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@excaliburjs/testing/dist/src/cli.js" "$@"
else
  exec node  "$basedir/../@excaliburjs/testing/dist/src/cli.js" "$@"
fi
